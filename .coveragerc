[run]
branch = True
omit = */tests.py
       */tests/*
       */migrations/*
       */management/*
       */asgi.py
       */wsgi.py
       */settings*.py
       */app/manage.py
       */utils/classes.py
       */utils/common.py

[report]
precision = 4
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__

[html]
directory = .pytest_cache/coverage
